<hibernate-mapping>
    <class name="in.monitor.authprovider.Clients" table="Clients">  
    	<id name="id">
    		<generator class="native"></generator>
    	</id>
    	<property name="Client_id" unique-key="true" not-null="true">
    	</property>
    	<property name="ClientSecret" ></property>
    	<property name="AuthCode" ></property>
    	<property name="AccessToken" ></property>
    	<property name="RefreshToken" ></property> 
    	<property name="CreatedAT" ></property>
    	<property name="ExpireTime" ></property>
    	
    	
    	
    	 </class>
</hibernate-mapping>      
   
