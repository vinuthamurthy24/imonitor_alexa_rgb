<hibernate-mapping>
    <class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.DeviceDetails" table="devicedetails">  
    	<!-- <cache usage="read-write"/> -->
    	<cache usage="read-write"/>
    	<id name="id" column="id">
    		<generator class="native"></generator>
    	</id>
    	<property name="did" unique="true"></property>
	    <property name="deviceName" unique="true"></property>
	    <property name="deviceIcon"></property>
    	<property name="gateway"></property>
    	<property name="locationName"></property>
    	<property name="locationId"></property>
    	<property name="enableList"></property>
    	<property name="deviceType"></property>
    	<property name="alertParam"></property>
    	<property name="customer"></property>
    	
    	
     </class>
</hibernate-mapping>  
