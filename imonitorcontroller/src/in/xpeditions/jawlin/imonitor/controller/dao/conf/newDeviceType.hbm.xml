<hibernate-mapping>
    <class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.NewDeviceType" table="newDeviceType">  
    	<id name="id" column="id">
    		<generator class="native"></generator>
    	</id>
    	<property name="deviceType"></property>
    	<property name="deviceTypeNumber"></property>
    	<property name="basicDeviceClass"></property>
    	<property name="genericDeviceClass"></property>
    	<property name="specificDeviceClass"></property>
    	<property name="supportedClass"></property>
    	<property name="manufacturerId"></property>
    	<property name="productId"></property>
    	<property name="modelNumber"></property>
     </class>
</hibernate-mapping>      
   
