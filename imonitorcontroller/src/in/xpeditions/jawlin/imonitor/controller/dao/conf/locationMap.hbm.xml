<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- Copyright Â© 2012 iMonitor Solutions India Private Limited -->

<hibernate-mapping>
    <class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.LocationMap" table="locationMap">  
    	<id name="id" column="id">
    		<generator class="native"></generator>
    	</id>
    	<property name="top"></property>
    	<property name="leftMap"></property>
    	<many-to-one name="device" class="in.xpeditions.jawlin.imonitor.controller.dao.entity.Device" lazy="proxy"></many-to-one>
     </class>
</hibernate-mapping>      
