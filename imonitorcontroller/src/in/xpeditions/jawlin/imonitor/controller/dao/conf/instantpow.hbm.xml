<hibernate-mapping>
    <class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.instantpow" table="instantpowerinformation">  
    	
    	<id name="id" column="id">
    		<generator class="native"></generator>
    	</id>
    	<property name="device"></property> 
    	<property name="alertType"></property>
    	<property name="alertTime"></property>
    	<property name="alarmStatus"></property>
        <property name="alertValue"></property>
    	<property name="alertTypeName"></property>
    	<property name="generatedDeviceId" unique="true"></property>
     </class>
</hibernate-mapping>
