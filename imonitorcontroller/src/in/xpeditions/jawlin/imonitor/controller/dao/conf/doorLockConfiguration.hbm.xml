<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.DoorLockConfiguration" table="doorlockconfiguration">  
<id name="id" column="id">
    		<generator class="native"></generator>
    	</id>
    	<many-to-one name="device" class="in.xpeditions.jawlin.imonitor.controller.dao.entity.Device" lazy="proxy"></many-to-one>
        <property name="userpassword"></property>
</class>

</hibernate-mapping>