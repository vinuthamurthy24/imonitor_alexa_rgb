<hibernate-mapping>
    <class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.ImvgLog" table="imvglog">  
    	<id name="id" column="id">
    	<generator class="native"></generator>
    	</id>
    	<property name="filename"></property>
    	<property name="filepath"></property>
    	<property name="uploadeddate"></property>
    	<many-to-one name="gateWay" class="in.xpeditions.jawlin.imonitor.controller.dao.entity.GateWay" lazy="proxy"></many-to-one>
     </class>
</hibernate-mapping>      
   
