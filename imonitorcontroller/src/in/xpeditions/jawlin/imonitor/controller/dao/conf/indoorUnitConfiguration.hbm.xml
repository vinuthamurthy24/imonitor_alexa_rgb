<hibernate-mapping>
    <class name="in.xpeditions.jawlin.imonitor.controller.dao.entity.IndoorUnitConfiguration" table="Indoor_Unit_Configuration">  
    	<id name="id" column="id">
    		<generator class="native"></generator>
    	</id>
    	<property name="Status"></property>
    	<property name="FanValue"></property>
    	<property name="FanDirection"></property>
    	<property name="Mode"></property>
    	<property name="Temperature"></property>
    	<property name="errorMessage"></property>
    	
     </class>
</hibernate-mapping>      
   
